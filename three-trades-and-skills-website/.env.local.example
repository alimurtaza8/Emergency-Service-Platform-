# Example environment variables - copy to .env.local and fill in your values

# Internet Computer Canister ID
NEXT_PUBLIC_CANISTER_ID=your_canister_id_here

# Internet Computer host (optional, defaults to mainnet)
# Use http://localhost:4943 for local development
NEXT_PUBLIC_IC_HOST=https://ic0.app

# Auth secret for session cookies
AUTH_SECRET=your_auth_secret_here

# Square Payment Integration
# Get these from the Square Developer Dashboard
NEXT_PUBLIC_SQUARE_APP_ID=your_square_app_id
NEXT_PUBLIC_SQUARE_LOCATION_ID=your_square_location_id
SQUARE_ACCESS_TOKEN=your_square_access_token

# Use true for sandbox environment, false for production
NEXT_PUBLIC_SQUARE_SANDBOX=true 